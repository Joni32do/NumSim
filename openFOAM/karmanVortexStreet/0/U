FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (3 0 0);

boundaryField
{
    in
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }

    out
    {
        //type            zeroGradient;

	type		inletOutlet;
	phi		phi;
	inletValue	uniform (3 0 0);
	value		uniform (3 0 0);
    }

    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    sym1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    sym2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    back
    {
        type            empty;
    }

    front
    {
        type            empty;
    }
}

