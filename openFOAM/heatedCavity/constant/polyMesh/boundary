(
    floor
    {
        type            wall;
        inGroups        1(wall);
        nFaces          20;
        startFace       760;
    }
    ceiling
    {
        type            wall;
        inGroups        1(wall);
        nFaces          20;
        startFace       780;
    }
    sideWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          40;
        startFace       800;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          800;
        startFace       840;
    }
